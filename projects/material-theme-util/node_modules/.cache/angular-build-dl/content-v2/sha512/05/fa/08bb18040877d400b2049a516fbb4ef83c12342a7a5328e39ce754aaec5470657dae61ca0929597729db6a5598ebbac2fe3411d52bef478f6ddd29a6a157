          /** @type {?} */

          var s = Math.round(_color.s);
          /** @type {?} */

          var l = Math.round(_color.l);

          if (isNaN(h) || isNaN(s) || isNaN(l)) {
            throw new TypeError("Invalid input");
          }

          if (h < 0 || h > 360) {
            throw new RangeError("Hue must be an integer within [0, 360]; given ".concat(h, "%"));
          }

