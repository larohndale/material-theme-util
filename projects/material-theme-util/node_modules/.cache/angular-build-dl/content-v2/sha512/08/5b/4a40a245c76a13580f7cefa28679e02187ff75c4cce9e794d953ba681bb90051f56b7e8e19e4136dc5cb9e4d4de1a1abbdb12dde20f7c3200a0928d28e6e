      var REGEX = {
        HEX: {
          PARSE: /^#?([a-fA-F0-9]{1,2})([a-fA-F0-9]{1,2})([a-fA-F0-9]{1,2})$/i,
          TEST: "^#?(([a-fA-F0-9]{2}){3}|([a-fA-F0-9]{1}){3})$"
        }
      };
      /*
      RGB Utilities
      =====================================================
      =====================================================
      */

      var rgb2Hex = function rgb2Hex(rgb) {
        var parseHex = function parseHex(v) {
          var hex = v.toString(16);
          return hex.length === 1 ? "0".concat(hex) : hex;
        };

        var _r = parseHex(rgb.r);

        var _g = parseHex(rgb.g);

        var _b = parseHex(rgb.b);

        return new Hex("#".concat(_r).concat(_g).concat(_b));
      };

      var rgb2Hsl = function rgb2Hsl(rgb) {
        var h = 0,
            s = 0,
            l = 0; // Make r, g, and b fractions of 1

        var _r = rgb.r /= 255;

        var _g = rgb.g /= 255;

        var _b = rgb.b /= 255; // Find greatest and smallest channel values


        var cmin = Math.min(_r, _g, _b),
            cmax = Math.max(_r, _g, _b),
            delta = cmax - cmin;
        if (delta == 0) h = 0; // Red is max
        else if (cmax == _r) h = (_g - _b) / delta % 6; // Green is max
          else if (cmax == _g) h = (_b - _r) / delta + 2; // Blue is max
            else h = (_r - _g) / delta + 4;
        h = Math.round(h * 60); // Make negative hues positive behind 360°

        if (h < 0) h += 360; // Calculate lightness

        l = (cmax + cmin) / 2; // Calculate saturation

        s = delta === 0 ? 0 : delta / (1 - Math.abs(2 * l - 1)); // Multiply l and s by 100

        s = +(s * 100).toFixed(1);
        l = +(l * 100).toFixed(1);
        return new HSL(h, s, l);
      };

      var ParseRGB = {
        HEX: rgb2Hex,
        HSL: rgb2Hsl
      };
      /*
      HEX Utilities
      =====================================================
      =====================================================
      */

      var hex2RGB = function hex2RGB(hex) {
        var r, g, b;
        var result = REGEX.HEX.PARSE.exec(hex.parse());

        if (!!result) {
          r = parseInt(result[1], 16);
          g = parseInt(result[2], 16);
          b = parseInt(result[3], 16);
        }

        return new RGB(r, g, b);
      };

      var hex2Hsl = function hex2Hsl(hex) {
        var rgb = hex2RGB(hex);
        return ParseRGB.HSL(rgb);
      };

      var testHex = function testHex(str) {
        return new RegExp(REGEX.HEX.TEST).test(str);
      };

      var ParseHEX = {
        RGB: hex2RGB,
        HSL: hex2Hsl,
        TEST: testHex
      };
      /*
      HSL Utilities
      =====================================================
      =====================================================
      */

      var hsl2RGB = function hsl2RGB(hsl) {
        // Must be fractions of 1
        var h = hsl.h;
        var s = hsl.s / 100;
        var l = hsl.l / 100;
        var c = (1 - Math.abs(2 * l - 1)) * s,
            x = c * (1 - Math.abs(h / 60 % 2 - 1)),
            m = l - c / 2;
        var r = 0,
            g = 0,
            b = 0;

        if (0 <= h && h < 60) {
          r = c;
          g = x;
          b = 0;
        } else if (60 <= h && h < 120) {
          r = x;
          g = c;
          b = 0;
        } else if (120 <= h && h < 180) {
          r = 0;
          g = c;
          b = x;
        } else if (180 <= h && h < 240) {
          r = 0;
          g = x;
          b = c;
        } else if (240 <= h && h < 300) {
          r = x;
          g = 0;
          b = c;
        } else if (300 <= h && h < 360) {
          r = c;
          g = 0;
          b = x;
        }

        r = Math.round((r + m) * 255);
        g = Math.round((g + m) * 255);
        b = Math.round((b + m) * 255);
        return new RGB(r, g, b);
      };

      var hsl2Hex = function hsl2Hex(hsl) {
        var rgb = hsl2RGB(hsl);
        return ParseRGB.HEX(rgb);
      };

      var ParseHSL = {
        RGB: hsl2RGB,
        HEX: hsl2Hex
      };
