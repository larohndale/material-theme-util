        function ColorPickerComponent() {
          _classCallCheck(this, ColorPickerComponent);

          this.regex = REGEX.HEX.TEST;

          this.parseHex = function (v) {
            if (v != null) {
              if (typeof v === "string") {
                v = parseInt(v);
              }

              var hex = v.toString(16);
              return hex.length > 1 ? hex : "".concat(hex).concat(hex);
            }

            return "FF";
          };
        }

