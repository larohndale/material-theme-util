      var Palette = function Palette(color, type) {
        var _this2 = this;

        _classCallCheck(this, Palette);

        this.color = color;
        this.type = type;

        this.buildPalette = function () {
          return _this2.generatePalette(_this2.color);
        };
        /*
         Generates a color palette given a single color.
                1. Color is parsed to HSL format
         2. Light (HS{L}) is altered for each Palette color
         3. Palette is generated with Color Objects (allowing for use in any color format)
               */


        this.generatePalette = function (color) {
          var _color = color.HSL;
          var h = Math.round(_color.h);
          var s = Math.round(_color.s);
          var l = Math.round(_color.l);

          if (isNaN(h) || isNaN(s) || isNaN(l)) {
            throw new TypeError("Invalid input");
          }

          if (h < 0 || h > 360) {
            throw new RangeError("Hue must be an integer within [0, 360]; given ".concat(h, "%"));
          }

          if (s < 0 || s > 100) {
            throw new RangeError("Saturation must be an integer within [0, 100]; given ".concat(s, "%"));
          }

          if (l < 0 || l > 100) {
            throw new RangeError("Lightness must be an integer within [0, 100]; given ".concat(l));
          }

          var hueMapset = _this2.generateAlterations(l).map(function (map) {
            var _color = new Color(new HSL(h, s, map.value));

